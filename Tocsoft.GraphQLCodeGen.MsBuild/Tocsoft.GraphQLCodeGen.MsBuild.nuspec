<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tocsoft.GraphQLCodeGen.MsBuild</id>
    <version>$version$</version>
    <authors>Tocsoft</authors>
    <description>msbuild target for generating clinet libraries from targeting graphql server.</description>
	<projectUrl>https://github.com/tocsoft/GraphQLCodeGen/</projectUrl>
	<repository type="git" url="https://github.com/tocsoft/GraphQLCodeGen/" />
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <file src="build\*.*" target="build" />
    <file src="buildMultiTargeting\*.*" target="buildMultiTargeting" />
    <file src="bin\$configuration$\**\*.*" target="tasks" />

    <file src="obj\binaries\net461\**\*.*" target="tasks\binaries\net461" />
    <file src="obj\binaries\netcoreapp3.1\**\*.*" target="tasks\binaries\netcoreapp3.1" />
  </files>
</package>
